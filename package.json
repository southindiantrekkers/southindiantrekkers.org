{
  "name": "southindiantrekkers",
  "version": "2.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "prestart": "pm2 start src/server/ecosystem.config.js --update-env",
    "start": "npm run build:dll:dev && webpack  --mode=development --watch --progress --colors",
    "dev": "npm run build:dll:dev && webpack --mode=development --progress",
    "prod": "npm run build:dll:prod && webpack  --mode=production --progress --colors",
    "build:dll:dev": "webpack --mode=development --config webpack.library.config.js",
    "build:dll:prod": "webpack --mode=production --config webpack.library.config.js",
    "serve": "pm2 start src/server/ecosystem.config.js --update-env",
    "test": "nyc mocha",
    "lint": "npm run jslint && npm run jsonlint && npm run csslint",
    "jslint": "eslint ./src/client ./src/server ./test/unit ./test/features --fix",
    "jsonlint": "eslint ./ --ext .json",
    "csslint": "stylelint ./src/client/**/*.scss --syntax scss --fix",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "heroku-postbuild": "npm run prod",
    "heroku": "npm run serve && pm2 logs"
  },
  "author": "",
  "license": "ISC",
